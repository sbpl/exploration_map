<launch>
    <include file="$(find exploration_map)/launch/exploration_map_node.launch"/> 
    <include file="$(find utacc_multimaster)/launch/segbot_gateway.launch"/> 
    <include file="$(find utacc_exploration)/launch/segbot_exploration_executive.launch"/>
    <include file="$(find ooi_detection)/launch/segbot_detection.launch"/>

    <node name="goal_relay" pkg="topic_tools" type="relay" args="segbot_active_goal goal"/>
    <node name="image_relay" pkg="image_transport" type="republish" args="compressed in:=segbot_cam/image_raw raw out:=segbot_cam"/>
    <node name="voltage_monitor" pkg="voltage_monitor" type="voltage_monitor"/>
</launch>
