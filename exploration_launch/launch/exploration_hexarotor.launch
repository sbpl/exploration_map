<launch>

<include file="$(find uav_launch)/launch/uav_launch.launch"/>

<include file="$(find exploration_map_node)/launch/exploration_map_node.launch"/> 

<include file="$(find utacc_multimaster)/launch/hexa_gateway.launch"/> 

<node name="hexarotor_status_pub" pkg="rostopic" type="rostopic" args="pub -r 1 /hexa_status utacc_msgs/MissionStatus '{}' "/>

<node name="fake_image_pub" pkg="occ" type="fake_image_pub" args="ooi_image_hexa"/>

<node name="goal_relay" pkg="topic_tools" type="relay" args="hexa_goal_e2 goal"/>

<node name="image_relay" pkg="image_transport" type="republish" args="compressed in:=hexa_cam/image_raw raw out:=hexa_cam"/>

</launch>
